          "options": [
            {
              "value": "top",
              "label": "t:sections.hero.blocks.hero-block.settings.align_vertical_mobile.options.option_1"
            },
            {
              "value": "center",
              "label": "t:sections.hero.blocks.hero-block.settings.align_vertical_mobile.options.option_2"
            },
            {
              "value": "bottom",
              "label": "t:sections.hero.blocks.hero-block.settings.align_vertical_mobile.options.option_3"
            }
          ],
          "default": "center"
        },
        {
          "type": "select",
          "id": "align_horizontal_mobile",
          "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal_mobile.label",
          "options": [
            {
              "value": "left",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal_mobile.options.option_1"
            },
            {
              "value": "center",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal_mobile.options.option_2"
            },
            {
              "value": "right",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal_mobile.options.option_3"
            }
          ],
          "default": "center"
        },
        {
          "type": "color",
          "id": "text_color",
          "label": "t:sections.collage-grid.blocks.block.settings.text_color.label"
        },
        {
          "type": "text",
          "id": "subheading",
          "label": "t:sections.global.settings.subheading.label",
          "default": "Add a tagline"
        },
        {
          "type": "richtext",
          "id": "heading",
          "label": "t:sections.global.settings.heading.label",
          "default": "<p>Slide heading with <em><strong>accent element</strong></em></p>"
        },
        {
          "type": "select",
          "id": "heading_size",
          "label": "t:sections.global.settings.heading_size.label",
          "options": [
            {
              "value": "h3",
              "label": "t:sections.global.settings.heading_size.options.option_1"
            },
            {
              "value": "h2",
              "label": "t:sections.global.settings.heading_size.options.option_2"
            },
            {
              "value": "h1",
