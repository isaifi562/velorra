          "default": false
        },
        {
          "type": "select",
          "id": "align_vertical",
          "label": "t:sections.hero.blocks.hero-block.settings.align_vertical.label",
          "options": [
            {
              "value": "top",
              "label": "t:sections.hero.blocks.hero-block.settings.align_vertical.options.option_1"
            },
            {
              "value": "center",
              "label": "t:sections.hero.blocks.hero-block.settings.align_vertical.options.option_2"
            },
            {
              "value": "bottom",
              "label": "t:sections.hero.blocks.hero-block.settings.align_vertical.options.option_3"
            }
          ],
          "default": "center"
        },
        {
          "type": "select",
          "id": "align_horizontal",
          "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal.label",
          "options": [
            {
              "value": "far_left",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal.options.option_4"
            },
            {
              "value": "left",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal.options.option_1"
            },
            {
              "value": "center",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal.options.option_2"
            },
            {
              "value": "right",
              "label": "t:sections.hero.blocks.hero-block.settings.align_horizontal.options.option_3"
